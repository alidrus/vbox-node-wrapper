{
    "name": "vbox-node-wrapper",
    "version": "0.5.0",
    "description": "CLI tool to simplify running and connecting to VirtualBox VMs written in NodeJS.",
    "browser": {
        "child_process": false,
        "fs": false
    },
    "bin": "src/vbox.js",
    "pkg": {
        "assets": [
            "package.json"
        ]
    },
    "scripts": {
        "dev": "yarn build-development",
        "prod": "yarn build-production",
        "build-development": "webpack --config webpack.config.js",
        "build-production": "pkg . --output dist/vbox",
        "installbin": "[ -e dist/vbox ] || yarn prod >& /dev/null && mkdir -p ${HOME}/bin && install -m 0755 dist/vbox ${HOME}/bin",
        "cleanup": "rm -fr dist/*"
    },
    "author": "Abu Bakar Al-Idrus",
    "license": "MIT",
    "dependencies": {
        "@babel/cli": "^7.10.4",
        "@babel/polyfill": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/register": "^7.10.4",
        "stringcase": "^4.3.1",
        "tty-table": "^4.1.3"
    },
    "devDependencies": {
        "pkg": "^4.4.9",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12"
    }
}
