{
    "name": "vbox-node-wrapper",
    "version": "0.2.0",
    "description": "CLI tool to simplify running and connecting to VirtualBox VMs written in NodeJS.",
    "browser": {
        "child_process": false,
        "fs": false
    },
    "scripts": {
        "dev": "yarn build-development",
        "prod": "yarn build-production",
        "build-development": "webpack --config webpack.config.js",
        "build-production": "pkg src/vbox.js --output dist/vbox"
    },
    "author": "Abu Bakar Al-Idrus",
    "license": "MIT",
    "dependencies": {
        "@babel/cli": "^7.10.4",
        "@babel/polyfill": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/register": "^7.10.4",
        "easy-table": "^1.1.1"
    },
    "devDependencies": {
        "pkg": "^4.4.9",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12"
    }
}
